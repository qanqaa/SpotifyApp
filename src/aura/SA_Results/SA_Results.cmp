    <lightning:card>
        <lightning:layout horizontalAlign="space" multipleRows="true">
            <lightning:layoutItem  size="12"
                                   mediumDeviceSize="6"
                                   padding="around-small">
                <c:SA_TrackResults/>
            </lightning:layoutItem>
            <lightning:layoutItem  size="12"
                                   mediumDeviceSize="6"
                                   padding="around-small">
                2
            </lightning:layoutItem>
        </lightning:layout>
    </lightning:card>
</aura:component>
